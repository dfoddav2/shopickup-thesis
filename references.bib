@article{papazoglouServiceOrientedArchitectures2007,
  title = {Service Oriented Architectures: Approaches, Technologies and Research Issues},
  shorttitle = {Service Oriented Architectures},
  author = {Papazoglou, Mike P. and family=Heuvel, given=Willem-Jan, prefix=van den, useprefix=true},
  date = {2007-07-01},
  journaltitle = {The VLDB Journal},
  shortjournal = {The VLDB Journal},
